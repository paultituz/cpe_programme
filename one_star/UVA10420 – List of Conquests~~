#include <iostream>
#include <vector>
#include <algorithm>
#include <sstream>
#include <ctype.h>
#include <string>
#include <cmath>
#include <iomanip>
#include <map>
//UVA10420 â€“ List of Conquests
using namespace std;


int main()
{
    int a;
    cin>>a;
    string input;
    getline(cin,input);
    map<string,int> country;
    while(a--)
    {


        getline(cin,input);

        stringstream ss(input);

        ss>>input;
        country[input]++;

    }

    for(auto i:country)
        {
            cout<<i.first<<" "<<i.second<<endl;
        }


    return 0;

}
